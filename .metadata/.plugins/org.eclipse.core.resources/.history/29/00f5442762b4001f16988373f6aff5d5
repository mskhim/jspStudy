package co.kh.dev.board.model;

import java.sql.Date;

public class BoardVO {
	
	private int rownum;          // Primary Key
    private String no;          // Primary Key
    private int memberNo;       // Foreign Key
    private String title;       // Not Null
    private String text;        // Not Null
    private int count;          // Default 0
    private Date subdate;       // Not Null
    
	public BoardVO(String no, int memberNo, String title, String text, int count, Date subdate) {
		super();
		this.no = no;
		this.memberNo = memberNo;
		this.title = title;
		this.text = text;
		this.count = count;
		this.subdate = subdate;
	}
	
	
	@Override
	public String toString() {
		return "BoardVO [no=" + no + ", memberNo=" + memberNo + ", title=" + title + ", text=" + text + ", count="
				+ count + ", subdate=" + subdate + "]";
	}


	public BoardVO(int memberNo, String title, String text) {
		super();
		this.memberNo = memberNo;
		this.title = title;
		this.text = text;
	}


	public BoardVO() {
		super();
	}
	
	
	
	public String getNo() {
		return no;
	}
	public void setNo(String no) {
		this.no = no;
	}
	public int getMemberNo() {
		return memberNo;
	}
	public void setMemberNo(int memberNo) {
		this.memberNo = memberNo;
	}
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getText() {
		return text;
	}
	public void setText(String text) {
		this.text = text;
	}
	public int getCount() {
		return count;
	}
	public void setCount(int count) {
		this.count = count;
	}
	public Date getSubdate() {
		return subdate;
	}
	public void setSubdate(Date subdate) {
		this.subdate = subdate;
	}
    
    

}
